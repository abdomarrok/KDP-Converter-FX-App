	<query language="SQL"><![CDATA[
   SELECT
    bc.id AS bon_commande_id,
    bc.numero AS bon_commande_numero,
    DATE(bc.date) AS bon_commande_date,
    bc.objet AS bon_commande_objet,
    bc.source_financement AS bon_commande_source_financement,
    bc.delai_execution AS bon_commande_delai_execution,
    bc.lieu_signature AS bon_commande_lieu_signature,
    bc.type_commande AS bon_commande_type,
    bc.command_exspense_type AS command_exspense_type,
    bc.status AS bon_commande_status,
    f.id AS fournisseur_id,
    f.name AS fournisseur_name,
    f.RC AS fournisseur_RC,
    f.NIF AS fournisseur_NIF,
    f.AI AS fournisseur_AI,
    f.NIS AS fournisseur_NIS,
    f.TEL AS fournisseur_TEL,
    f.FAX AS fournisseur_FAX,
    f.ADDRESS AS fournisseur_ADDRESS,
    f.EMAIL AS fournisseur_EMAIL,
    f.RIB AS fournisseur_RIB,
    a.id AS article_id,
    a.name AS article_name,
    a.unite AS article_unite,
    ci.quantite AS quantity,
    ci.prix_unitaire AS unit_price,
    e.name AS enterprise_name,
    e.address AS enterprise_address,
    e.phone AS enterprise_phone,
    e.fax AS enterprise_fax,
    e.email AS enterprise_email,
    e.logo AS enterprise_logo,
    e.ministry_name AS ministry_name,
    e.daira_name AS daira_name,
    e.wilaya_name AS wilaya_name
FROM bon_commande bc
LEFT JOIN fournisseur f ON bc.id_fournisseur = f.id
LEFT JOIN commande_item ci ON bc.id = ci.id_bc
LEFT JOIN article a ON ci.id_article = a.id
LEFT JOIN enterprise_info e ON e.id = 1
WHERE bc.id = $P{bon_commande_id};
]]></query>
